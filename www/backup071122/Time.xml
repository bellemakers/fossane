<?xml version="1.0" encoding="UTF-8"?>
<calendar-time xmlns:xvrl="http://www.xvrl.org">
	<xvrl:resolver>
		<xvrl:name>timeResolver</xvrl:name>
		<xvrl:class>xvrl/resolver/time/TimeResolver</xvrl:class>
	</xvrl:resolver>
	<date>
		<year>
			<xvrl:value>
				<xvrl:resolver-name>timeResolver</xvrl:resolver-name>
				<xvrl:method>getYear</xvrl:method>
			</xvrl:value>
		</year>
		<month>
			<xvrl:value>
				<xvrl:resolver-name>timeResolver</xvrl:resolver-name>
				<xvrl:method>getMonth</xvrl:method>
			</xvrl:value>
		</month>
		<day>
			<xvrl:value>
				<xvrl:resolver-name>timeResolver</xvrl:resolver-name>
				<xvrl:method>getDay</xvrl:method>
			</xvrl:value>
		</day>
	</date>
	<time>
		<hour>
			<xvrl:value>
				<xvrl:resolver-name>timeResolver</xvrl:resolver-name>
				<xvrl:method>getHour</xvrl:method>
			</xvrl:value>
		</hour>
		<minute>
			<xvrl:value>
				<xvrl:resolver-name>timeResolver</xvrl:resolver-name>
				<xvrl:method>getMinute</xvrl:method>
			</xvrl:value>
		</minute>
		<second>
			<xvrl:value>
				<xvrl:resolver-name>timeResolver</xvrl:resolver-name>
				<xvrl:method>getSecond</xvrl:method>
			</xvrl:value>
		</second>
		<millisecond>
			<xvrl:value>
				<xvrl:resolver-name>timeResolver</xvrl:resolver-name>
				<xvrl:method>getMillisecond</xvrl:method>
			</xvrl:value>
		</millisecond>
	</time>
</calendar-time>
